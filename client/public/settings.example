# settings for the AURORA WEB-Client
#
aurora.rest.server    : "myauroraserver.mydomain:9393"

# some general settings
www.domain              : "mydomain"
www.base                : "https://mydomain"
www.cookiename          : "MyDomainAuroraWebClient"
www.cookie.timeout      : 31536000
www.timeout             : 36000
www.tree.timeout        : 120
www.makeannouncements   : true
www.helppages           : "/docs/user/"
www.webmaster           : "orakel@ntnu.no"
www.maintenance         : false


# dataset cache in Manage-view
dataset.cache.size      : 100000

# oauth login details
oauth.script            : "/feide.cgi"

# gatekeeper ipv4 resolver
gatekeeper.ipv4resolver          : "https://ipv4resolverdomain:8055"

privacy.www.questions            : "https://mydomain/whereever/questions.html"
privacy.www.ombud                : "https://mydomain/whereever/contacts.html"
